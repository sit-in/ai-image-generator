# Supabase Configuration (Required)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Replicate API (Required)
REPLICATE_API_TOKEN=your_replicate_token
REPLICATE_MODEL=black-forest-labs/flux-schnell

# Security (Required for production)
# CSRF_SECRET should be at least 32 characters long
# Generate with: openssl rand -hex 32
CSRF_SECRET=your-strong-random-secret-here-min-32-chars
NODE_ENV=development

# Optional: MCP Configuration
SUPABASE_PROJECT_REF=your_project_ref
SUPABASE_ACCESS_TOKEN=your_access_token

# Optional: API Configuration
NEW_API_KEY=your_api_key
NEW_API_BASE_URL=your_api_base_url
NEXT_PUBLIC_NEW_API_BASE_URL=your_public_api_base_url

# Optional: Performance
PNPM_VERSION=10.10.0

# Redis Configuration (Highly recommended for production)
# Required for: rate limiting, session management, multi-instance deployment
# Format: redis://[username]:[password]@[host]:[port]/[database]
REDIS_URL=redis://localhost:6379

# CDN Configuration (optional)
# IMAGE_CDN_URL=https://your-cdn.com

# File Upload Limits
# MAX_FILE_SIZE=10485760  # 10MB in bytes